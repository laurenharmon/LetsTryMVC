
@using LetsTryMVC.Models
@model List<CartItem>

<h1>Cart</h1>

<table class="table">
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Quantity</th>
        <th scope="col">Price</th>
        <th scope="col">Product</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Quantity</td>
            <td>@item.Product.Price</td>
            <td>@item.Product.Name</td>
        </tr>

    }
</table>